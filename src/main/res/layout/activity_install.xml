<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            android:orientation="vertical"
            android:background="@color/white"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <include layout="@layout/title1"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/zhanghu"
                android:textColor="@color/black"
                android:textSize="14sp"
                android:layout_marginTop="11dp"
                android:layout_marginLeft="@dimen/left"
                />
            <RelativeLayout
                android:id="@+id/install_changepass_rl"
                android:layout_marginTop="13dp"
                android:background="@drawable/gray_rond_back"
                android:layout_marginLeft="@dimen/left"
                android:layout_marginRight="@dimen/right"
                android:layout_width="match_parent"
                android:layout_height="@dimen/hight">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="5dp"
                    android:text="@string/changepass"
                    android:textColor="@color/black"
                    android:textSize="14sp"
                    />
                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="5dp"
                    android:src="@mipmap/mine_toright"
                    />
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/install_hei_rl"
                android:layout_marginTop="11dp"
                android:background="@drawable/gray_rond_back"
                android:layout_marginLeft="@dimen/left"
                android:layout_marginRight="@dimen/right"
                android:layout_width="match_parent"
                android:layout_height="@dimen/hight">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="5dp"
                    android:text="@string/heimingd"
                    android:textColor="@color/black"
                    android:textSize="14sp"
                    />
                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="5dp"
                    android:src="@mipmap/mine_toright"
                    />
            </RelativeLayout>
            <View
                android:layout_marginTop="26dp"
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="@color/e6" />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/install"
                android:layout_gravity="right"
                android:layout_marginTop="15dp"
                android:layout_marginRight="@dimen/right"
                android:textColor="@color/black"
                android:textSize="14sp"
                />
            <RelativeLayout
                android:id="@+id/install_langueage_rl"
                android:layout_marginTop="10dp"
                android:background="@drawable/gray_rond_back"
                android:layout_marginLeft="@dimen/left"
                android:layout_marginRight="@dimen/right"
                android:layout_width="match_parent"
                android:layout_height="@dimen/hight">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="5dp"
                    android:text="@string/langueage"
                    android:textColor="@color/black"
                    android:textSize="14sp"
                    />
                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="5dp"
                    android:src="@mipmap/mine_toright"
                    />
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/install_tuisong_rl"
                android:layout_marginTop="11dp"
                android:background="@drawable/gray_rond_back"
                android:layout_marginLeft="@dimen/left"
                android:layout_marginRight="@dimen/right"
                android:layout_width="match_parent"
                android:layout_height="@dimen/hight">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="5dp"
                    android:text="@string/tuisongins"
                    android:textColor="@color/black"
                    android:textSize="14sp"
                    />
                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="5dp"
                    android:src="@mipmap/mine_toright"
                    />
            </RelativeLayout>
            <RelativeLayout
                android:visibility="gone"
                android:layout_marginTop="11dp"
                android:background="@drawable/gray_rond_back"
                android:layout_marginLeft="@dimen/left"
                android:layout_marginRight="@dimen/right"
                android:layout_width="match_parent"
                android:layout_height="@dimen/hight">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="5dp"
                    android:text="@string/needoldpic"
                    android:textColor="@color/black"
                    android:textSize="14sp"
                    />
                <ImageView
                    android:id="@+id/install_changepic_iv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="5dp"
                    android:src="@mipmap/open_pic"
                    />
            </RelativeLayout>
            <View
                android:layout_marginTop="26dp"
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="@color/e6" />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="14dp"
                android:layout_marginLeft="@dimen/left"
                android:text="@string/zhichi"
                android:textColor="@color/black"
                android:textSize="14sp"
                />
            <RelativeLayout
                android:id="@+id/install_help_rl"
                android:layout_marginTop="11dp"
                android:background="@drawable/gray_rond_back"
                android:layout_marginLeft="@dimen/left"
                android:layout_marginRight="@dimen/right"
                android:layout_width="match_parent"
                android:layout_height="@dimen/hight">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="5dp"
                    android:text="@string/userxieyi"
                    android:textColor="@color/black"
                    android:textSize="14sp"
                    />
                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="5dp"
                    android:src="@mipmap/mine_toright"
                    />
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/install_qustion_rl"
                android:layout_marginTop="11dp"
                android:background="@drawable/gray_rond_back"
                android:layout_marginLeft="@dimen/left"
                android:layout_marginRight="@dimen/right"
                android:layout_width="match_parent"
                android:layout_height="@dimen/hight">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="5dp"
                    android:text="@string/upqustion"
                    android:textColor="@color/black"
                    android:textSize="14sp"
                    />
                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:layout_marginRight="5dp"
                    android:src="@mipmap/mine_toright"
                    />
            </RelativeLayout>
            <View
                android:layout_marginTop="26dp"
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:background="@color/e6" />
            <RelativeLayout
            android:id="@+id/install_cleansou_rl"
            android:layout_marginTop="11dp"
            android:background="@drawable/gray_rond_back"
            android:layout_marginLeft="@dimen/left"
            android:layout_marginRight="@dimen/right"
            android:layout_width="match_parent"
            android:layout_height="@dimen/hight">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="5dp"
                android:text="@string/cleanhistorysou"
                android:textColor="@color/black"
                android:textSize="14sp"
                />
        </RelativeLayout>
            <RelativeLayout
                android:id="@+id/install_cleanliao_rl"
                android:layout_marginTop="11dp"
                android:background="@drawable/gray_rond_back"
                android:layout_marginLeft="@dimen/left"
                android:layout_marginRight="@dimen/right"
                android:layout_width="match_parent"
                android:layout_height="@dimen/hight">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="5dp"
                    android:text="@string/cleanhistoryliao"
                    android:textColor="@color/black"
                    android:textSize="14sp"
                    />
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/install_exit_rl"
                android:layout_marginTop="11dp"
                android:background="@drawable/gray_rond_back"
                android:layout_marginLeft="@dimen/left"
                android:layout_marginRight="@dimen/right"
                android:layout_width="match_parent"
                android:layout_height="@dimen/hight">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="5dp"
                    android:text="@string/backall"
                    android:textColor="@color/black"
                    android:textSize="14sp"
                    />
            </RelativeLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="30dp"/>
        </LinearLayout>
    </ScrollView>
</LinearLayout>